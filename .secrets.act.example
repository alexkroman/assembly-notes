# Secrets file for act
# Format: SECRET_NAME=secret_value
# Copy this to .secrets.act.local and fill in your values

# GitHub tokens
GITHUB_TOKEN=your_github_token
PAT_TOKEN=your_personal_access_token_with_repo_permissions

# Apple credentials for notarization
APPLE_ID=your_apple_id@example.com
APPLE_APP_SPECIFIC_PASSWORD=your_app_specific_password
APPLE_TEAM_ID=your_team_id
CSC_LINK=base64_encoded_certificate
CSC_KEY_PASSWORD=certificate_password